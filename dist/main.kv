<ActivitiesCalendarScreen>:
    orientation: "vertical"
    spacing: 10
    space_x: self.size[0]/3
  
    canvas.before:
        Color:
            rgba: (1, 1, 1, 1)
        Rectangle:
            source:'Backgrounds/ABG.png'
            size: root.width, root.height
    
    MDCard:
        orientation: "horizontal"
        pos_hint: {"center_x": .5, "center_y": .97}
        size_hint: 1, .085
        md_bg_color: rgba("#372B78")
        MDCard:
            elevation: 0
            padding: [dp(10),dp(15),dp(50),dp(7)]
            size_hint: .75, 1
            md_bg_color: rgba("#372B78")
            MDTextButton: 
                text: "StudyStar"
                on_release: app.home()
                font_name:"LobsterBold"
                font_size: 64
                theme_text_color: "Custom"
                text_color: 1,1,1,1    
        MDCard:
            elevation: 0
            size_hint: .25, 1
            md_bg_color: rgba("#372B78")
            on_release: app.show_email_dialog()
            Image:
                size_hint: .75, .75
                pos_hint: {"center_x": .5, "center_y": .375}
                source: "Icons/Email.png"

    MDCard:
        id: profile_cd
        orientation: "horizontal"
        pos_hint: {"center_x": .5, "center_y": .825}
        size_hint: .95, .15
        on_release: app.profile ()
        md_bg_color: rgba("#372B78")
        radius: [35]
        ThreeLineListItem:
            id: profile_cd_lst
            markup: True
            theme_text_color: "Custom"
            text_color: 1,1,1,1
            secondary_theme_text_color: 'Custom'    
            secondary_text_color: 1,1,1,1
            tertiary_theme_text_color: 'Custom'
            tertiary_text_color: 1,1,1,1 
            text: "[b][/b]"
            secondary_text: "ORCHARD FARM HIGH SCHOOL"
            tertiary_text: "Grade 10"
        FitImage:
            id: profile_cd_img
            size_hint_x: .35
            size_hint_y: 1
            radius: [35]

