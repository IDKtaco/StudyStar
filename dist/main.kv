            height: 60
            Button:
                text: "Photo Library"
                on_release: root.photo()
            Button:
                text: "Take Photo"
                on_release: root.camera()
            Button:
                text: "Choose File"
                on_release: root.show_load_list()


<LoadDialog>:
    BoxLayout:
        size: root.size
        pos: root.pos
        orientation: "vertical"
        FileChooserIconView:
            id: filechooser
            path: './'
        BoxLayout:
            size_hint_y: None
            height: 30
            Button:
                text: "Cancel"
                on_release: root.cancel()
            Button:
                text: "Load"
                on_release:
                    root.img(filechooser.path, filechooser.selection)
                    root.show_result()


